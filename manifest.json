{
    "icons": {
        "16": "frontend/images/icon.png",
        "32": "frontend/images/icon.png",
        "48": "frontend/images/icon.png",
        "128": "frontend/images/icon.png"
    },
    "name": "Youtube Overlay",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": "frontend/images/icon.png"
        
    },
    "permissions": [
        "contextMenus",
        "sidePanel",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },

    "side_panel": {
        "default_path": "frontend/sidepanel.html"
    },

    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          }
        }
      }

 




  

    
}